{
    "name": "react-quantlib",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build ; cp-cli build/index.html build/404.html",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build -r quantlib-wasm-demo"
    },
    "dependencies": {
        "@captor/react-scripts": "3.1.1-a",
        "quantlib-wasm": "^1.16.1",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "cpy-cli": "^2.0.0",
        "gh-pages": "^2.1.1",
        "eslint-config-prettier": "6.0.0"
    },
    "prettier": {
        "printWidth": 160,
        "semi": true,
        "tabWidth": 4,
        "bracketSpacing": true,
        "arrowParens": "always"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
